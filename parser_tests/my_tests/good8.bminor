factorial: function integer ( x: integer ) = {
    if (x < 0) return -1;
    if (x < 3) return x;
    return x * factorial(x-1);
}

main: function integer ( argc: integer, argv: array [] string ) = {

    print "Hello, world!\n";
    x: integer = 4;
    print x, " factorial is ", factorial(x), "\n";
    return -0;
}
